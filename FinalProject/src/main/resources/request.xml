<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="requestns">
<resultMap type="request" id="requestResult">
	<result property="requestCode" 		column="requestCode"/>
	<result property="subject" 		column="subject"/>
	<result property="content" 		column="content"/>
	<result property="requestDate" 		column="requestDate"/>
	<result property="memberCode" 		column="memberCode"/>
	<result property="companyCode" 		column="companyCode"/>
	<result property="read" 		column="read"/>
</resultMap>

</mapper>



